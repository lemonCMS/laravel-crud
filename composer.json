{
    "name": "lemoncms/laravel-crud",
    "description": "rest api crud plugin for laravel 6",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "raymond",
            "email": "rsvvuuren@hotmail.com"
        }
    ],
    "require": {},
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^4.6",
        "laravel/framework": "^6.16",
        "guzzlehttp/guzzle": "^6.5",
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3",
        "facade/ignition": "^1.16"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LemonCMS\\LaravelCrud\\ServiceProvider"
            ],
            "aliases": {
                "Debugbar": "LemonCMS\\LaravelCrud\\Facade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "LemonCMS\\LaravelCrud\\": "src",
            "TestApp\\": "tests\\TestApp",
            "App\\": "tests\\App"
        }
    },
    "scripts": {
        "fix": [
            "php-cs-fixer fix"
        ]
    }
}
